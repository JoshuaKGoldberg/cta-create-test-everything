	"files": [
		"LICENSE.md",
		"README.md",
		"lib/",
		"package.json"
	],
	"scripts": {
		"build": "tsup",
		"format": "prettier .",
		"lint": "eslint . --max-warnings 0",
		"lint:knip": "knip",
