		"prettier-plugin-packagejson": "latest",
		"prettier-plugin-sh": "latest",
		"sentences-per-line": "latest",
		"tsup": "latest",
		"typescript": "latest",
		"typescript-eslint": "latest",
		"vitest": "latest"
	},
	"engines": {
		"node": ">=18.3.0"
	},
	"publishConfig": {
		"provenance": true
	}
